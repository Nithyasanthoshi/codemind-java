import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner christ = new Scanner(System.in);
		int X = christ.nextInt();
		if(X == 25){
		    System.out.println("CHRISTMAS");
		}
		else{
		    System.out.println("ORDINARY");
		}

	}
}
