import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc = new Scanner(System.in);
    String s = sc.nextLine();
    int index = 6;
    char luckyLetter = s.charAt(index);
    System.out.println(luckyLetter);
		

	}
}
