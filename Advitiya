import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner read = new Scanner(System.in);
		int N = read.nextInt();
		if(N == 16 || N == 17 || N==18){
		    System.out.println(" ADVITIYA");
		}
		else{
		    System.out.println(" WAITING FOR ADVITIYA");
		}

	}
}
